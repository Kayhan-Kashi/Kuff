        </th>
        <th>
            @Html.DisplayNameFor(model => model.DateOfAdding)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Summary)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Comments)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ProductTypeCode)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ProductTypeDateOfCreation)
