            @Html.DisplayFor(modelItem => item.Comments)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ProductTypeCode)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ProductTypeDateOfCreation)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ProductTypeComments)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ProductTypeTitle)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Id }) |
            @Html.ActionLink("Details", "Details", new { id=item.Id }) |
